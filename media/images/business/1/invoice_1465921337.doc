	    function change_cost(cost, to) {
		switch (to) {
		    case 'usd':
			return  Math.ceil(cost / 1620 * 1) / 1; //we need 2dp in us $
			break;
		    default:
			return cost;
			break;
		}
	    }
	</script> 
